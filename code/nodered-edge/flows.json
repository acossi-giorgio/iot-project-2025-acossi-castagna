    {
        "id": "fe04938caa982bd3",
        "type": "function",
        "z": "b667b205bc64704e",
        "name": "prepare zadd request",
        "func": "const p = msg.payload || {};\nconst key = msg.topic.replace(/\\//g, \":\");\n\nconst score = p.event_ts ? Date.parse(p.event_ts) : Date.now();\nif (!Number.isFinite(score)) return null;\n\nconst member = JSON.stringify({\n  ts: p.event_ts,\n  type: p.type,\n  unit: p.unit,\n  value: p.value,\n  sessionId: p.sessionId\n});\n\nmsg.payload = [ key, String(score), member ];\ndelete msg.topic;\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
